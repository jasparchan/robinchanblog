Host github.com
    User git
    StrictHostKeyChecking no
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host git.oschina.net
	User git
	StrictHostKeyChecking no
	IdentityFile ~/.ssh/id_rsa
	IdentitiesOnly yes

Host robinchan.cn
	User git
	StrictHostKeyChecking no
	IdentityFile ~/.ssh/id_rsa
	IdentitiesOnly yes